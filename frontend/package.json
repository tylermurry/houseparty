{
  "name": "houseparty",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "gen:api": "openapi-ts -i ../HouseParty.Server/openapi/houseparty-server.json -o src/api-client/client",
    "format": "prettier --write --experimental-cli src/",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "validate": "run-s gen:api lint type-check build",
    "dev": "vite",
    "build": "vite build"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7",
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.90.10",
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.8.1",
    "sass-embedded": "^1.97.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.2"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
